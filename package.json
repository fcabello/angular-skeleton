{
  "name": "angular-skeleton",
  "version": "0.0.0",
  "description": "Simple angular app with express server backing it for rapid prototyping and testing.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "bower install && gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mbildner/angular-skeleton.git"
  },
  "author": "Moshe Bildner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mbildner/angular-skeleton/issues"
  },
  "homepage": "https://github.com/mbildner/angular-skeleton",
  "devDependencies": {
    "gulp": "^3.8.10",
    "gulp-angular-templatecache": "^1.5.0",
    "gulp-concat": "^2.4.2",
    "gulp-jshint": "^1.9.0",
    "gulp-ng-annotate": "^0.4.3",
    "main-bower-files": "^2.4.1",
    "require-dir": "^0.1.0",
    "rimraf": "^2.2.8"
  },
  "dependencies": {
    "body-parser": "^1.10.0",
    "cookie-parser": "^1.3.3",
    "express": "^4.10.6",
    "yargs": "^1.3.3"
  },
  "jshintConfig": {
    "browser": true,
    "globals": {
      "angular": false
    },
    "maxerr": 10000,
    "strict": true,
    "undef": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "nonew": true,
    "quotmark": "single",
    "unused": true
  }
}
